{
  "manifest_version": 3,
  "name": "Search and Video Tracker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "history",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "src/background.ts"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.ts"]
    }
  ]
}